{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "bracketSpacing": true,
  "semi": true,
  "printWidth": 120,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "avoid",
  "quoteProps": "consistent",
  "vueIndentScriptAndStyle": false,
  "htmlWhitespaceSensitivity": "ignore"
}
